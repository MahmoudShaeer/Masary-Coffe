    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/PanCakelike2"
        android:layout_below="@+id/PanCakemediam2price"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <View
