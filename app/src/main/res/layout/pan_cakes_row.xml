        android:layout_alignLeft="@+id/PanCaketextView5"
        android:layout_alignStart="@+id/PanCaketextView5"
        android:id="@+id/PanCaketextView6" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20"
        android:textSize="15dp"
