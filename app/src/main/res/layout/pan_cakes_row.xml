    <TextView
        android:id="@+id/PanCakelarg2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="L"
        android:textColor="#e9ffb923"
        android:textSize="19sp"
        android:layout_alignBaseline="@+id/PanCakemediam2"
        android:layout_alignBottom="@+id/PanCakemediam2"
        android:layout_alignLeft="@+id/PanCakecost_L2"
        android:layout_alignStart="@+id/PanCakecost_L2" />

    <TextView
        android:id="@+id/PanCakemediam2price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="25"
        android:textColor="#5b5b5b"
        android:textSize="15dp"
        android:layout_below="@+id/PanCakelarg2"
        android:layout_toRightOf="@+id/PanCakelike2"
        android:layout_toEndOf="@+id/PanCakelike2" />
